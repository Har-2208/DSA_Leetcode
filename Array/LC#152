int maxProduct(int* nums, int numsSize) {
    int maxProd = nums[0];
    int minProd = nums[0];
    int result  = nums[0];

    for (int i = 1; i < numsSize; i++) {
        if (nums[i] < 0) {
            int temp = maxProd;
            maxProd = minProd;
            minProd = temp;
        }

        if(nums[i]>nums[i]*maxProd){
            maxProd = nums[i];
        }
        else{
            maxProd = nums[i]*maxProd;
        }
        if(nums[i]<nums[i]*minProd){
            minProd = nums[i];
        }
        else{
            minProd = nums[i]*minProd;
        }
        

        if (result < maxProd)
            result = maxProd;
    }

    return result;
}
