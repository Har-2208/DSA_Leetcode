bool canConstruct(char* ransomNote, char* magazine) {
    int freq[26] = {0};
    int n = strlen(magazine);
    int m = strlen(ransomNote);
    for(int i=0; i<n; i++){
        freq[magazine[i] - 'a']++;
    }
    for(int i=0; i<m; i++){
        freq[ransomNote[i] - 'a']--;
    }

    for(int i=0; i<m; i++){
        if(freq[ransomNote[i]-'a']<0){
            return false;
        }
    }

    return true;
}